{
  "name": "@piloting/web-components",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button/index.d.ts",
      "import": "./dist/button/index.js"
    },
    "./button/react": {
      "types": "./dist/button/react.d.ts",
      "import": "./dist/button/react.js"
    },
    "./button/angular": {
      "types": "./dist/button/angular.d.ts",
      "import": "./dist/button/angular.js"
    },
    "./text-field": {
      "types": "./dist/text-field/index.d.ts",
      "import": "./dist/text-field/index.js"
    },
    "./text-field/react": {
      "types": "./dist/text-field/react.d.ts",
      "import": "./dist/text-field/react.js"
    },
    "./text-field/angular": {
      "types": "./dist/text-field/angular.d.ts",
      "import": "./dist/text-field/angular.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./angular": {
      "types": "./dist/angular/index.d.ts",
      "import": "./dist/angular/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "clean": "rm -rf dist coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "lit": "^3.1.0"
  },
  "peerDependencies": {
    "@angular/core": ">=17.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@angular/core": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular/core": "^17.3.0",
    "@types/bun": "^1.1.0",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json"
}
