{
  "name": "@oxi-ui/core",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./authentication": {
      "types": "./dist/authentication/index.d.ts",
      "import": "./dist/authentication/index.js"
    },
    "./feature-flags": {
      "types": "./dist/feature-flags/index.d.ts",
      "import": "./dist/feature-flags/index.js"
    },
    "./permissions": {
      "types": "./dist/permissions/index.d.ts",
      "import": "./dist/permissions/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist coverage",
    "format:check": "oxfmt --check src",
    "format": "oxfmt --write src",
    "lint:fix": "oxlint src --fix",
    "lint": "oxlint src",
    "prepublishOnly": "bun run build",
    "test:coverage": "bun test --coverage",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0"
  }
}
